{
  "system": "dubin",
  "experiment_type": "barrier_function",
  "strategy": "neural_network",
  "dynamics": {
    "mu": 0.0,
    "sigma": 0.02,
    "num_samples": 200,
    "dt": 0.1,
    "velocity": 1.0,
    "horizon": 40,
    "initial_set": "front",
    "unsafe_set": "barrel"
  },
  "partitioning": {
    "method": "grid",
    "num_slices": [1, 1, 1]
  },
  "model": {
    "type": "fcnn",
    "input_dim": 3,
    "hidden_nodes": 128,
    "hidden_layers": 3,
    "activation": "relu"
  },
  "training": {
    "batch_size": 1000,
    "iter_per_epoch": 400,
    "eps": [0.01, 0.01, 0.01],
    "epochs": 150,
    "test_every": 25,
    "adjust_overlap": false
  },
  "test": {
    "ibp_batch_size": 30,
    "crown_ibp_batch_size": 30
  }
}