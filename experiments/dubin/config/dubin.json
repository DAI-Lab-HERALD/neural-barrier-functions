{
  "system": "dubin",
  "experiment_type": "barrier_function",
  "dynamics": {
    "sigma": 0.01,
    "num_samples": 200,
    "dt": 0.1,
    "velocity": 1.0,
    "horizon": 2
  },
  "partitioning": {
    "method": "grid",
    "num_slices": [80, 80, 45]
  },
  "model": {
    "type": "fcnn",
    "input_dim": 3,
    "hidden_nodes": 64,
    "hidden_layers": 3,
    "activation": "relu"
  },
  "training": {
    "batch_size": 500,
    "iter_per_epoch": 400,
    "eps": [0.05, 0.05, 0.06981317007],
    "epochs": 150,
    "empirical_epochs": 20,
    "test_every": 1,
    "adjust_overlap": true
  },
  "test": {
    "ibp_batch_size": 2000,
    "crown_ibp_batch_size": 400
  }
}