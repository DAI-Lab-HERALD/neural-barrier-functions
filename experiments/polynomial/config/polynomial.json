{
  "system": "polynomial",
  "experiment_type": "barrier_function",
  "dynamics": {
    "sigma": 0.1,
    "num_samples": 500,
    "dt": 0.1,
    "horizon": 10
  },
  "partitioning": {
    "method": "grid",
    "num_slices": [80, 80]
  },
  "model": {
    "type": "fcnn",
    "input_dim": 2,
    "hidden_nodes": 128,
    "hidden_layers": 3,
    "activation": "relu"
  },
  "training": {
    "batch_size": 1000,
    "iter_per_epoch": 400,
    "eps": [0.005, 0.005],
    "epochs": 150,
    "empirical_epochs": 20,
    "test_every": 5,
    "adjust_overlap": false
  },
  "test": {
    "ibp_batch_size": 15,
    "crown_ibp_batch_size": 15
  }
}