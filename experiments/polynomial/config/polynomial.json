{
  "system": "polynomial",
  "dynamics": {
    "sigma": 0.1,
    "num_samples": 500,
    "dt": 0.5,
    "horizon": 10
  },
  "partitioning": {
    "method": "grid",
    "num_slices": [320, 320]
  },
  "model": {
    "type": "fcnn",
    "input_dim": 2,
    "hidden_nodes": 32,
    "hidden_layers": 3,
    "activation": "relu"
  },
  "training": {
    "batch_size": 200,
    "iter_per_epoch": 400,
    "eps": 0.0375,
    "epochs": 150,
    "empirical_epochs": 50,
    "test_every": 1
  },
  "test": {
    "ibp_batch_size": 500,
    "crown_ibp_batch_size": 50
  }
}