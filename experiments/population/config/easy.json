{
  "system": "population",
  "dynamics": {
    "sigma": 0.1,
    "fertility_rate": 0.2,
    "survival_juvenile": 0.3,
    "survival_adult": 0.8,
    "num_samples": 500,
    "horizon": 10,
    "safe_set": "circle"
  },
  "partitioning": {
    "method": "grid",
    "num_slices": [160, 160]
  },
  "model": {
    "type": "fcnn",
    "input_dim": 2,
    "hidden_nodes": 256,
    "hidden_layers": 5,
    "activation": "relu"
  },
  "training": {
    "batch_size": 200,
    "iter_per_epoch": 200,
    "eps": 0.0375,
    "epochs": 150,
    "test_every": 25
  },
  "test": {
    "ibp_batch_size": 500,
    "crown_ibp_batch_size": 100
  }
}