{
  "system": "population",
  "dynamics": {
    "sigma": 0.1,
    "fertility_rate": 1.0,
    "survival_juvenile": 0.5,
    "survival_adult": 0.5,
    "num_samples": 500,
    "horizon": 10,
    "safe_set": "annulus"
  },
  "partitioning": {
    "method": "grid",
    "num_slices": [320, 320]
  },
  "model": {
    "type": "fcnn",
    "input_dim": 2,
    "hidden_nodes": 128,
    "hidden_layers": 3,
    "activation": "relu"
  },
  "training": {
    "batch_size": 1000,
    "iter_per_epoch": 400,
    "eps": 0.00703125,
    "epochs": 150,
    "empirical_epochs": 20,
    "test_every": 1
  },
  "test": {
    "ibp_batch_size": 500,
    "crown_ibp_batch_size": 100
  }
}