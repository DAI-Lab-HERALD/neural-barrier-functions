{
  "system": "population",
  "experiment_type": "barrier_function",
  "dynamics": {
    "sigma": 0.1,
    "fertility_rate": 0.4,
    "survival_juvenile": 0.3,
    "survival_adult": 0.8,
    "num_samples": 100,
    "horizon": 10,
    "safe_set": "circle"
  },
  "partitioning": {
    "method": "grid",
    "num_slices": [4, 4]
  },
  "model": {
    "type": "fcnn",
    "input_dim": 2,
    "hidden_nodes": 10,
    "hidden_layers": 2,
    "activation": "relu"
  },
  "training": {
    "batch_size": 100,
    "iter_per_epoch": 400,
    "eps": [0.00001, 0.00001],
    "epochs": 50,
    "test_every": 25,
    "adjust_overlap": false,
    "empirical_only": false
  },
  "test": {
    "ibp_batch_size": 1,
    "crown_ibp_batch_size": 1
  }
}