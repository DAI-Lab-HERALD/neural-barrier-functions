{
  "system": "population",
  "experiment_type": "barrier_function",
  "dynamics": {
    "sigma": 0.1,
    "fertility_rate": 0.2,
    "survival_juvenile": 0.4,
    "survival_adult": 0.8,
    "num_samples": 500,
    "horizon": 10,
    "safe_set": "circle"
  },
  "partitioning": {
    "method": "grid",
    "num_slices": [4, 4]
  },
  "model": {
    "type": "fcnn",
    "input_dim": 2,
    "hidden_nodes": 128,
    "hidden_layers": 3,
    "activation": "relu"
  },
  "training": {
    "batch_size": 1000,
    "iter_per_epoch": 400,
    "eps": 0.001,
    "epochs": 150,
    "empirical_epochs": 20,
    "test_every": 5,
    "adjust_overlap": false
  },
  "test": {
    "ibp_batch_size": 100,
    "crown_ibp_batch_size": 100
  }
}