{
  "system": "population",
  "dynamics": {
    "sigma": 0.1,
    "fertility_rate": 0.2,
    "survival_juvenile": 0.6,
    "survival_adult": 0.9,
    "num_samples": 500,
    "horizon": 2,
    "safe_set": "circle"
  },
  "partitioning": {
    "method": "grid",
    "num_slices": [640, 640]
  },
  "model": {
    "type": "fcnn",
    "input_dim": 2,
    "hidden_nodes": 512,
    "hidden_layers": 5,
    "activation": "relu"
  },
  "training": {
    "batch_size": 200,
    "iter_per_epoch": 200,
    "eps": 0.01,
    "epochs": 150,
    "test_every": 25
  },
  "test": {
    "ibp_batch_size": 500,
    "crown_ibp_batch_size": 50
  }
}