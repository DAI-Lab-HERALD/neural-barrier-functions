{
  "system": "population",
  "dynamics": {
    "sigma": 0.1,
    "fertility_rate": 1.0,
    "survival_juvenile": 0.5,
    "survival_adult": 0.5,
    "num_samples": 500,
    "horizon": 10,
    "safe_set": "donut"
  },
  "partitioning": {
    "method": "grid",
    "num_slices": [160, 160]
  },
  "model": {
    "type": "fcnn",
    "input_dim": 2,
    "hidden_nodes": 256,
    "hidden_layers": 3,
    "activation": "relu"
  },
  "training": {
    "batch_size": 400,
    "epochs": 300,
    "status": {
      "ibp_batch_size": 500,
      "crown_ibp_batch_size": 100
    }
  }
}