{
  "system": "linear",
  "experiment_type": "barrier_function",
  "dynamics": {
    "sigma": [0.1, 0.1],
    "num_samples": 500,
    "horizon": 10,
    "safe_set": "circle"
  },
  "partitioning": {
    "method": "grid",
    "num_slices": [4, 4]
  },
  "model": {
    "type": "fcnn",
    "input_dim": 2,
    "hidden_nodes": 128,
    "hidden_layers": 3,
    "activation": "relu"
  },
  "training": {
    "batch_size": 1000,
    "iter_per_epoch": 400,
    "eps": [0.0001, 0.0001],
    "epochs": 150,
    "test_every": 25,
    "adjust_overlap": false,
    "empirical_only": false
  },
  "test": {
    "ibp_batch_size": 1,
    "crown_ibp_batch_size": 1
  }
}